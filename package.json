    "element-ui": "^2.13.2",
    "inquirer": "^6.5.2",
    "lodash": "^4.17.14",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinyin-match": "^1.0.9",
    "svg-sprite-loader": "^4.1.6",
    "vue": "2.6.11",
    "vue-awesome": "^3.5.4",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.5.0",
    "vue-router": "3.4.3",
    "vue-runtime-helpers": "^1.1.2",
