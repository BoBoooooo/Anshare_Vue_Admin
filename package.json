    "@vue/cli-service": "3.9.3",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "vuetify-loader": "1.2.2",
    "eslint": "6.1.0",
    "eslint-plugin-vue": "5.2.3",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
