  "scripts": {
    "start": "vue-cli-service serve --mode dev",
    "build:out": "vue-cli-service build --mode out",
    "build:dev": "vue-cli-service build --mode dev",
    "report:out": "vue-cli-service build --mode report",
    "lint": "vue-cli-service lint --mode dev",
