    "vue-cli-plugin-commitlint": "^1.0.12",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "2.6.11",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "right-pad": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue,ts}": [
      "npm run lint",
      "git add"
    ]
