    "echarts": "^4.2.1",
    "element-ui": "2.10.1",
    "js-cookie": "2.2.0",
    "jsoneditor": "^6.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "screenfull": "4.2.0",
    "script-loader": "^0.7.2",
    "svg-sprite-loader": "^4.1.6",
    "vue": "2.6.10",
    "vue-awesome": "3.5.4",
    "vue-chartjs": "^3.4.2",
    "vue-router": "3.0.7",
    "vuedraggable": "2.23.0",
